<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link width="" rel="shortcut icon" href="/images/download.png" />
	<link width="" rel="shortcut icon" href="/images/download.png" />

	<title> Cars PRO </title>
	<!-- Font Awesome Stylesheet -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
	<!-- Bootstrap Grid Stylesheet -->
	<link rel="stylesheet" href="/stylesheets/bootstrap-grid.min.css">
	<!-- Own Stylesheet -->
	<link rel="stylesheet" href="/stylesheets/app.css">
	
</head>

<body>

	<% if (usuarioLogueado) { %> 
		<%- include('./partials/headerLogueado.ejs') %>
		<% } else { %>
			<%- include('./partials/headerNoLogueado.ejs') %>
		<% } %>
	
	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title"> 
					<%= title %> 
				</h2>
			</div>
			<!-- Show the "ultimos" products -->
			
		</div>

		<div class="container products-wrapper">
			<div class="row">
				
				<!-- Show the "ultimos" products -->
				<% lista.forEach(element => {%>
					<img src="<%= element.imagen %>" alt="">
				<% }) %>
			</div>
		</div>

		<div class="container products-wrapper">
			<div class="row">
				
				<!-- Show the "ultimos" products -->
				<% lista.forEach(element => {%>
					<img src="<%= element.descripcion %>" alt="">
				<% }) %>
			</div>
		</div>
	</div>
<br>
<br>
<br>
<br>
<br>
